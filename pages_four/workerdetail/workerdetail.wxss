.two {
    width: 100%;
    padding: calc(20vmin / 7.5) 0;
    border: 1rpx solid #F2F3F5;
}

.two>view {
    width: 95%;
}

.line>view>view {
    margin: calc(10vmin / 7.5) 0;
    display: flex;
