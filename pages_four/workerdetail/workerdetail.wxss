.lines {
    width: 100%;
    margin: calc(10vmin / 7.5) 0;
}

.lines>view {
    width: calc(100% - 80rpx);
    display: flex;
    justify-content: space-between;
}

.star {
    width: 24rpx;
    height: 24rpx;
}

.c-line .btn {
    width: 120rpx;
    height: 40rpx;
