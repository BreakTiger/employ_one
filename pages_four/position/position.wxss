.f50 {
  font-size: 50rpx;
}

page {
  background: #f5f5f5;
}

.container {
  height: 100%;
}

.container>view {
  width: 100%;
}

.condition {
  height: 70rpx;
  border-bottom: 1rpx solid #ececec;
  position: fixed;
  top: 0rpx;
  z-index: 100;
}

.condition>view {
  width: 49%;
  height: 100%;
  text-align: center;
}

.condition>view>text {
  width: 88%;
}

.icon {
  width: 26rpx;
  height: 26rpx;
}

.content {
  position: fixed;
  top: 72rpx;
  z-index: 100;
}

.item {
  width: 94%;
  padding: 15rpx 0;
  margin: 0 auto;
  border-bottom: 1rpx solid #ececec;
}

.choice {
  width: 36rpx;
  height: 36rpx;
}

.title {
  width: 100%;
  height: 75rpx;
  line-height: 75rpx;
  background: #f5f5f5;
  color: #616161;
  text-indent: 20rpx;
  font-size: 22rpx;
}

.list {
  border-bottom: 1rpx solid #F6F6F6;
}

.one {
  width: 30%;
  height: 245rpx;
  border-right: 1rpx solid #F6F6F6;
  background: #fdfdfd;
}

.two {
  width: 69%;
}

.two>view {
  width: 90%;
}

.btn>view {
  width: 94rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  border: 2rpx solid #f5f5f5;
  border-radius: 5rpx;
  font-size: 20rpx;
  color: #5F5F5F;
  margin-left: 15rpx;
}


.add {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  position: fixed;
  bottom: 0rpx;
  z-index: 100;
}