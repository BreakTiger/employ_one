.title {
  width: 100%;
  height: 75rpx;
  line-height: 75rpx;
  background: #f5f5f5;
  font-size: 22rpx;
  color: #616161;
}

.title>text {
  margin-left: 30rpx;
}

.picture {
  min-height: 350rpx;
}

.picture>image,
.picture>view {
  width: 121rpx;
  height: 121rpx;
  margin-left: 25rpx;
  margin-top: 30rpx;
  position: relative;
}

.close {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  width: 50rpx;
  height: 50rpx;
  z-index: 200;
  background: black;
  border-radius: 50%;
}

.videos>image,
.videos>view {
  width: 698rpx;
  height: 433rpx;
  margin-top: 35rpx;
  position: relative;
}

.close_one {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  width: 50rpx;
  height: 50rpx;
  z-index: 200;
  background: white;
  border-radius: 50%;
}

.add {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  bottom: 0rpx;
  z-index: 100;
}

.emedia{
  width: 351rpx;
  height: 352rpx;
  margin-top: 200rpx;
}